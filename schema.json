{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "properties": {
    "color": {
      "description": "Bank brand color",
      "required": true,
      "type": "string"
    },
    "country": {
      "description": "Two letters code of bank country (ISO 3166-1 alpha-2)",
      "required": true,
      "type": "string"
    },
    "engTitle": {
      "description": "Bank name in English",
      "required": true,
      "type": "string"
    },
    "localTitle": {
      "description": "Bank name in local language",
      "required": true,
      "type": "string"
    },
    "name": {
      "description": "Unique project code for the bank",
      "required": true,
      "type": "string"
    },
    "prefixes": {
      "items": {
        "minimum": 10000,
        "type": "number"
      },
      "minItems": 0,
      "required": true,
      "type": "array",
      "uniqueItems": true
    },
    "url": {
      "description": "Link to official bank website",
      "required": true,
      "type": "string"
    }
  },
  "title": "Bank schema",
  "type": "object"
}